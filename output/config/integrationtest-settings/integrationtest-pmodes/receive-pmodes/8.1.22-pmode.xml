<?xml version="1.0"?>
<PMode 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns="eu:edelivery:as4:pmode">
  <Id>8.1.22-pmode</Id>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReplyHandling>
    <ReplyPattern>Response</ReplyPattern>
    <SendingPMode>unsigned-response-pmode</SendingPMode>
    <ReceiptHandling>
      <UseNRRFormat>false</UseNRRFormat>
    </ReceiptHandling>
    <ErrorHandling>
      <UseSoapFault>false</UseSoapFault>
      <ResponseHttpCode>500</ResponseHttpCode>
    </ErrorHandling>
  </ReplyHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>true</NotifyMessageConsumer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\exceptions\" />
      </Parameters>
    </NotifyMethod>
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Ignored</Signature>
    </SigningVerification>
    <Decryption>
      <Encryption>Ignored</Encryption>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>AccessPointB</CertificateFindValue>
      </CertificateFindCriteria>
    </Decryption>
  </Security>
  <MessagePackaging>
    <PartyInfo>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>org:eu:europa:as4:example:accesspoint:B</Id>
          </PartyId>
        </PartyIds>
        <Role>Receiver</Role>
      </ToParty>
    </PartyInfo>
    <CollaborationInfo>
      <AgreementReference>
        <Value>http://agreements.holodeckb2b.org/examples/agreement0</Value>
      </AgreementReference>
      <Action>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/test</Action>
      <Service>
        <Value>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/service</Value>
        <Type>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/service</Type>
      </Service>
    </CollaborationInfo>
  </MessagePackaging>
  <MessageHandling>
    <Forward>
      <SendingPMode>8.1.22-pmode</SendingPMode>
    </Forward>
  </MessageHandling>
</PMode>