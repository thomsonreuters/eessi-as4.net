        </PartyIds>
        <Role>urn:eu:europa:ec:dgempl:eessi:ir:institution</Role>
      </FromParty>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>UK:UK010</Id>
            <Type>urn:eu:europa:ec:dgempl:eessi:ir</Type>
          </PartyId>
        </PartyIds>
        <Role>urn:eu:europa:ec:dgempl:eessi:ir:institution</Role>
      </ToParty>
    </PartyInfo>
    <CollaborationInfo>
      <Service>
        <Value>BusinessMessaging</Value>
        <Type>urn:eu:europa:ec:dgempl:eessi</Type>
      </Service>
      <Action>Send</Action>
    </CollaborationInfo>
    <Mpc>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/defaultMPC/UK:UK001</Mpc>
  </MessagePackaging>
</PMode>