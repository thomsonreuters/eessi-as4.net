<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="eu:edelivery:as4:pmode">
  <Id>receiveagent-non_existing_decrypt_cert-pmode</Id>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReplyHandling>
    <ReplyPattern>Response</ReplyPattern>
    <SendingPMode>unsigned-response-pmode</SendingPMode>
    <ReceiptHandling>
      <UseNRRFormat>false</UseNRRFormat>
    </ReceiptHandling>
    <ErrorHandling>
      <UseSoapFault>false</UseSoapFault>
      <ResponseHttpCode>200</ResponseHttpCode>
    </ErrorHandling>
  </ReplyHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>true</NotifyMessageConsumer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\exceptions\" />
      </Parameters>
    </NotifyMethod>
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Ignored</Signature>
    </SigningVerification>
    <Decryption>
      <Encryption>Allowed</Encryption>
	  <CertificateFindCriteria>
		<CertificateFindType>FindBySubjectName</CertificateFindType>
		<CertificateFindValue>nonexisting_subjectname</CertificateFindValue>
	  </CertificateFindCriteria>
    </Decryption>
  </Security>
  <MessagePackaging>    
    <CollaborationInfo>
      <AgreementReference>
        <Value>http://agreements.europa.org/agreement</Value>
      </AgreementReference>
      <Action>as4.net:receive_agent:decryption_failed</Action>
      <Service>
        <Value>errorhandling</Value>
        <Type>as4.net:receive_agent:componenttest</Type>
      </Service>
    </CollaborationInfo>
  </MessagePackaging>
  <MessageHandling>
    <Deliver>
      <IsEnabled>true</IsEnabled>
      <PayloadReferenceMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
        </Parameters>
      </PayloadReferenceMethod>
      <DeliverMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
        </Parameters>
      </DeliverMethod>
    </Deliver>
  </MessageHandling>
</PMode>