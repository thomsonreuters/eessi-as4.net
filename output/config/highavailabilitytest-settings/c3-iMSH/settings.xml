	    <ForwardAgent name="Forward Agent">
            <Receiver type="Eu.EDelivery.AS4.Receivers.DatastoreReceiver, Eu.EDelivery.AS4.Receivers, Version=3.1.0.0, Culture=neutral, PublicKeyToken=null">
                <Setting key="Table">InMessages</Setting>
                <Setting key="Filter">Operation = 'ToBeForwarded'</Setting>
                <Setting key="Update" field="Operation">Forwarding</Setting>
            </Receiver>        
            <Transformer type="Eu.EDelivery.AS4.Transformers.ForwardMessageTransformer, Eu.EDelivery.AS4.Transformers, Version=3.1.0.0, Culture=neutral, PublicKeyToken=null" />            
            <StepConfiguration>
                <NormalPipeline>
                    <Step type="Eu.EDelivery.AS4.Steps.Forward.DetermineRoutingStep, Eu.EDelivery.AS4.Steps, Version=3.1.0.0, Culture=neutral, PublicKeyToken=null" />
                    <Step type="Eu.EDelivery.AS4.Steps.Submit.DynamicDiscoveryStep, Eu.EDelivery.AS4.Steps, Version=3.1.0.0, Culture=neutral, PublicKeyToken=null" />
                    <Step type="Eu.EDelivery.AS4.Steps.Forward.CreateForwardMessageStep, Eu.EDelivery.AS4.Steps, Version=3.1.0.0, Culture=neutral, PublicKeyToken=null" />                 
                </NormalPipeline>
                <ErrorPipeline />                    
            </StepConfiguration>
        </ForwardAgent>
    </Agents>
</Settings>