<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="eu:edelivery:as4:pmode">
  <Id>PING</Id>
  <Mep>OneWay</Mep>
  <MepBinding>Push</MepBinding>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReceiptHandling>
    <UseNNRFormat>false</UseNNRFormat>
    <ReplyPattern>Response</ReplyPattern>
    <SendingPMode>Minder_response</SendingPMode>
  </ReceiptHandling>
  <ErrorHandling>
    <NotifyMessageConsumer>false</NotifyMessageConsumer>
    <NotifyMethod />
    <UseSoapFault>false</UseSoapFault>
    <ReplyPattern>Response</ReplyPattern>
    <ResponseHttpCode>500</ResponseHttpCode>
    <SendingPMode>Minder_response</SendingPMode>
  </ErrorHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>false</NotifyMessageConsumer>
    <NotifyMethod />
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Allowed</Signature>
    </SigningVerification>
  <Signing>
      <IsEnabled>false</IsEnabled>
      <KeyReferenceMethod>IssuerSerial</KeyReferenceMethod>
      <PrivateKeyFindValue>as4-net-cs2</PrivateKeyFindValue>
      <PrivateKeyFindType>FindBySubjectName</PrivateKeyFindType>
      <Algorithm>http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</Algorithm>
      <HashFunction>http://www.w3.org/2001/04/xmlenc#sha256</HashFunction>
    </Signing>
    <Decryption>
      <Encryption>Allowed</Encryption>
      <PrivateKeyFindValue>as4-net-cs2</PrivateKeyFindValue>
      <PrivateKeyFindType>FindBySubjectName</PrivateKeyFindType>
    </Decryption>
  </Security>
  <MessagePackaging>
    <PartyInfo>
        <FromParty>
          <PartyIds>
            <PartyId>
              <Id>as4-net-c2</Id>
            </PartyId>
          </PartyIds>
          <Role>Sender</Role>
        </FromParty>
        <ToParty>
          <PartyIds>
            <PartyId>
              <Id>as4-net-c3</Id>
            </PartyId>
          </PartyIds>
          <Role>Receiver</Role>
        </ToParty>
      </PartyInfo>
      <CollaborationInfo>
        <Action>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/test</Action>
        <Service>
          <Value>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/service</Value>
        </Service>
    </CollaborationInfo>
  </MessagePackaging>
  <Deliver>
    <IsEnabled>true</IsEnabled>
     <PayloadReferenceMethod >
      <Type>FILE</Type>
      <Parameters>
          <Parameter name="location" value=".\messages\in\"/>
        </Parameters>
    </PayloadReferenceMethod>
    <DeliverMethod>
      <Type>FILE</Type>
      <Parameters>
          <Parameter name="location" value=".\messages\in\"/>
        </Parameters>
    </DeliverMethod>
  </Deliver>
</PMode>