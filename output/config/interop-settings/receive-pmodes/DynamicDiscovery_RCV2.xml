<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="eu:edelivery:as4:pmode">
  <Id>DYNAMIC_DISCOVERY_RCV_AS4NET-C2</Id>
  <Mep>OneWay</Mep>
  <MepBinding>Push</MepBinding>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReplyHandling>
    <ReplyPattern>Response</ReplyPattern>
    <SendingPMode>SIGNED_RESPONSE_FROM_C2</SendingPMode>
    <ReceiptHandling>
      <UseNRRFormat>true</UseNRRFormat>
    </ReceiptHandling>
    <ErrorHandling>
      <UseSoapFault>false</UseSoapFault>
      <ResponseHttpCode>200</ResponseHttpCode>
    </ErrorHandling>
  </ReplyHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>true</NotifyMessageConsumer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\exceptions\" />
      </Parameters>
    </NotifyMethod>
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Allowed</Signature>
    </SigningVerification>
    <Decryption>
      <Encryption>Allowed</Encryption>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>as4-net-c3</CertificateFindValue>
      </CertificateFindCriteria>
    </Decryption>
  </Security>
  <MessagePackaging>
    <PartyInfo>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>as4.net-c2</Id>
            <Type />
          </PartyId>
        </PartyIds>
        <Role>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/responder</Role>
      </ToParty>
    </PartyInfo>
    <CollaborationInfo>
      <Action>ACT_SIMPLE_ONEWAY_DYN</Action>
      <Service>
        <Value>SRV_SIMPLE_ONEWAY_DYN</Value>
      </Service>
    </CollaborationInfo>
  </MessagePackaging>
  <MessageHandling>
    <Deliver>
      <IsEnabled>true</IsEnabled>
      <PayloadReferenceMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
        </Parameters>
      </PayloadReferenceMethod>
      <DeliverMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
        </Parameters>
      </DeliverMethod>
    </Deliver>
  </MessageHandling>
</PMode>