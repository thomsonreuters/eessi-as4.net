<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="eu:edelivery:as4:pmode">
    <Id>c2-receive-pmode</Id>
    <Mep>OneWay</Mep>
    <MepBinding>Push</MepBinding>
    <ReplyHandling>
        <ReplyPattern>Callback</ReplyPattern>
        <SendingPMode>c2-push-pmode</SendingPMode>
        <ReceiptHandling>
            <UseNNRFormat>true</UseNNRFormat>
        </ReceiptHandling>
        <ErrorHandling />
    </ReplyHandling>
    <Security>
        <SigningVerification>
            <Signature>Allowed</Signature>
        </SigningVerification>
        <Decryption>
            <Encryption>Ignored</Encryption>
            <PrivateKeyFindType>FindByThumbprint</PrivateKeyFindType>
        </Decryption>
    </Security>
    <MessagePackaging>
        <PartyInfo>
            <FromParty>
                <PartyIds>
                    <PartyId>
                        <Id>UK:UK010</Id>
                        <Type>urn:eu:europa:ec:dgempl:eessi:ir</Type>
                    </PartyId>
                </PartyIds>
                <Role>urn:eu:europa:ec:dgempl:eessi:ir:institution</Role>
            </FromParty>
            <ToParty>
                <PartyIds>
                    <PartyId>
                        <Id>UK:UK001</Id>
                        <Type>urn:eu:europa:ec:dgempl:eessi:ir</Type>
                    </PartyId>
                </PartyIds>
                <Role>urn:eu:europa:ec:dgempl:eessi:ir:institution</Role>
            </ToParty>
        </PartyInfo>
        <CollaborationInfo>
            <Service>
                <Value>BusinessMessaging</Value>
                <Type>urn:eu:europa:ec:dgempl:eessi</Type>                
            </Service>
            <Action>Send</Action>
        </CollaborationInfo>
    </MessagePackaging>
    <Deliver>
        <IsEnabled>true</IsEnabled>
        <PayloadReferenceMethod>
            <Type>FILE</Type>
            <Parameters>
                <Parameter name="location" value=".\messages\in\" />
            </Parameters>
        </PayloadReferenceMethod>
        <DeliverMethod>
            <Type>FILE</Type>
            <Parameters>
                <Parameter name="location" value=".\messages\in\" />
            </Parameters>
        </DeliverMethod>
    </Deliver>
</PMode>